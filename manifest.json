{
	"manifest_version": 2,
	"name": "Dom-Mapper",
	"description": "Run a one-line script to visually map DOM elements on a view",
	"version": "1.4",
	"author": "mlande",
	"browser_action": {
		"default_icon": "chrome_store/icons/icon.png",
		"default_title": "Dom Mapper"
	},
	"background": {
		"scripts": [ "map-dom.js" ],
		"persistent": false
	},
	"permissions": [ "activeTab" ]
}
